    "@sendgrid/mail": "^8.1.4",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "prisma": "^6.3.1",
    "stripe": "^17.5.0",
    "twilio": "^5.4.3",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
